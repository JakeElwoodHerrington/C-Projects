#include <iostream>
#include <fstream>

using namespace std;

void bubbleSort(string a[]) {
	bool isSorted = false;

	while (!isSorted) {
		isSorted = true;
		for (int i = 0; i < 45; i++) {
			if (a[i].compare(a[i + 1]) > 0) {
				string temp = a[i];
				a[i] = a[i + 1];
				a[i + 1] = temp;
				isSorted = false;

			}
		}
	}

int main() {

	ifstream fin;
	fin.open("presidents.txt");
	string presidents[46];
	ofstream fout;
	fout.open("presidents_sorted.txt");
	for (int i = 0; i < 46; i++) {
		fout << presidents << endl;
	}


	return 0;
}



}
